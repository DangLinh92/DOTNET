@model List<KeKhaiBaoHiemViewModel>

<h3 class="card-title">
    Thông tin chi trả bảo hiểm
    <a href="#" class="edit-icon" data-toggle="modal" data-target="#add_kkbh_info_modal" id="btn-addKeKhaiBHInfo"><i class="fa fa-plus"></i></a>
</h3>
<table class="table table-striped custom-table datatable" id="keKhaiBHDataTable">
    <thead>
        <tr>
            <th>Chế Độ</th>
            <th>Ngày Bắt Đầu</th>
            <th>Ngày Kết Thúc</th>
            <th>Ngày Thanh Toán</th>
            <th>Số Tiền Thanh Toán</th>
            <th class="text-right no-sort">Action</th>
        </tr>
    </thead>
    <tbody id="tbl-content">
        @foreach (var nv in Model)
        {
            <tr>
                <td>@nv.HR_CHEDOBH?.TenCheDo</td>
                <td>@nv.NgayBatDau</td>
                <td>@nv.NgayKetThuc</td>
                <td>@nv.NgayThanhToan</td>
                <td>@nv.SoTienThanhToan</td>
                <td class="text-right">
                    <div class="dropdown dropdown-action">
                        <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="material-icons">more_vert</i></a>
                        <div class="dropdown-menu dropdown-menu-right">
                            <a class="dropdown-item edit-kkbh" href="#" data-toggle="modal" data-target="#edit_kkbh" data-id="@nv.Id"><i class="fa fa-pencil m-r-5"></i> Edit</a>
                            <a class="dropdown-item delete-kkbh" href="#" data-toggle="modal" data-target="#delete_kkbh" data-id="@nv.Id"><i class="fa fa-trash-o m-r-5"></i> Delete</a>
                        </div>
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>


