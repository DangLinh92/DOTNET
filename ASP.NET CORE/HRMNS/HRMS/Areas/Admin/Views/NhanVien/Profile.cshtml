@model NhanVienProfileModel
@{
    ViewData["Title"] = "Profile";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section Scripts
{
    <!-- Timeline JS -->
    <script src="~/plugins/timeline/horizontal-timeline.js"></script>
    <script src="~/lib/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/app/controllers/nhanvien/profile-managerment.js"></script>
    <script>
        var profileMng = new ProfileMng();
        profileMng.initialize();
    </script>
}

@section Styles{
    <style>
        .page-wrapper {
            min-height: 641px;
        }
    </style>
}
<!-- Page Content -->
<div class="content container-fluid">

    <!-- Page Header -->
    <div class="page-header">
        <div class="row">
            <div class="col-sm-12">
                <h3 class="page-title">Profile</h3>
                <ul class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/admin/nhanvien/index">Employees</a></li>
                    <li class="breadcrumb-item active">Profile</li>
                </ul>
            </div>
        </div>
    </div>
    <!-- /Page Header -->

    <div class="card mb-0">
        <div class="card-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="profile-view">
                        <div class="profile-img-wrap">
                            <div class="profile-img">
                                <a id="btnSelectImg" href="#"><img alt="" id="imgAvatar" src="@Model.Avartar"></a>
                                <input type="file" id="fileInputImage" style="display:none" />
                            </div>
                        </div>
                        <div class="profile-basic" id="id-profile-basic">
                            @await Html.PartialAsync("_profilebasicPartial", Model)
                        </div>
                        <div class="pro-edit"><a data-target="#profile_info" data-toggle="modal" class="edit-icon" href="#" id="btn-UpdateProfileBasic" data-id="@Model.MaNhanVien"><i class="fa fa-pencil"></i></a></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="card tab-box">
        <div class="row user-tabs">
            <div class="col-lg-12 col-md-12 col-sm-12 line-tabs">
                <ul class="nav nav-tabs nav-tabs-bottom">
                    <li class="nav-item"><a href="#emp_profile" data-toggle="tab" class="nav-link active">Sơ Yếu Lý Lịch</a></li>
                    <li class="nav-item"><a href="#emp_projects" data-toggle="tab" class="nav-link">Tình Trạng Hồ Sơ</a></li>
                    <li class="nav-item"><a href="#quatrinhCongTacTab" data-toggle="tab" class="nav-link">Quá Trình Làm Việc</a></li>
                    <li class="nav-item"><a href="#bank_statutory" data-toggle="tab" class="nav-link">Hợp Đồng <small class="text-danger">(Admin Only)</small></a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="tab-content">

        <!-- Profile Info Tab -->
        <div id="emp_profile" class="pro-overview tab-pane fade show active">
            <div class="row">
                <div class="col-md-12 d-flex">
                    <div class="card profile-box flex-fill">
                        <div class="card-body">
                            <h3 class="card-title">Personal Informations <a href="#" class="edit-icon" data-toggle="modal" data-target="#personal_info_modal"><i class="fa fa-pencil"></i></a></h3>
                            <ul class="personal-info">
                                <li>
                                    <div class="title">Nơi Sinh.</div>
                                    <div class="text">@Model.NoiSinh</div>
                                </li>
                                <li>
                                    <div class="title">Nguyên Quán.</div>
                                    <div class="text">@Model.NguyenQuan</div>
                                </li>
                                <li>
                                    <div class="title">Địa Chỉ Thường Trú</div>
                                    <div class="text">@Model.DiaChiThuongTru</div>
                                </li>
                                <li>
                                    <div class="title">Dân Tộc</div>
                                    <div class="text">@Model.DanToc</div>
                                </li>
                                <li>
                                    <div class="title">Tôn Giáo</div>
                                    <div class="text">@Model.TonGiao</div>
                                </li>
                                <li>
                                    <div class="title">CMTND/CCCD</div>
                                    <div class="text">@Model.CMTND</div>
                                </li>
                                <li>
                                    <div class="title">Ngày cấp CMTND</div>
                                    <div class="text">@Model.NgayCapCMTND</div>
                                </li>
                                <li>
                                    <div class="title">Nơi cấp CMTND</div>
                                    <div class="text">@Model.NoiCapCMTND</div>
                                </li>
                                <li>
                                    <div class="title">Mã Số Thuế</div>
                                    <div class="text">@Model.MaSoThue</div>
                                </li>
                                <li>
                                    <div class="title">Trường Đào Tạo</div>
                                    <div class="text">@Model.TruongDaoTao</div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

            </div>
            <div class="row">
                <div class="col-md-6 d-flex">
                    <div class="card profile-box flex-fill">
                        <div class="card-body">
                            <h3 class="card-title">Bank information<a href="#" class="edit-icon" data-toggle="modal" data-target="#bank_infor_modal"><i class="fa fa-pencil"></i></a></h3>
                            <ul class="personal-info">
                                <li>
                                    <div class="title">Bank name</div>
                                    <div class="text">@Model.TenNganHang</div>
                                </li>
                                <li>
                                    <div class="title">Bank account No.</div>
                                    <div class="text">@Model.SoTaiKhoanNH</div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 d-flex">
                    <div class="card profile-box flex-fill">
                        <div class="card-body">
                            <h3 class="card-title">Emergency Contact <a href="#" class="edit-icon" data-toggle="modal" data-target="#emergency_contact_modal"><i class="fa fa-pencil"></i></a></h3>
                            <ul class="personal-info">
                                <li>
                                    <div class="title">Quan hệ</div>
                                    <div class="text">@Model.QuanHeNguoiThan</div>
                                </li>
                                <li>
                                    <div class="title">Số Điện Thoại</div>
                                    <div class="text">@Model.SoDienThoaiNguoiThan</div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 d-flex">
                    <div class="card profile-box flex-fill">
                        <div class="card-body">
                            <h3 class="card-title">Social Insurance<a href="#" class="edit-icon" data-toggle="modal" data-target="#bhxh_infor_modal"><i class="fa fa-pencil"></i></a></h3>
                            <ul class="personal-info">
                                <li>
                                    <div class="title">Mã BHXH</div>
                                    <div class="text">@Model.bHXHs?.Id</div>
                                </li>
                                <li>
                                    <div class="title">Ngày Tham Gia</div>
                                    <div class="text">@Model.bHXHs?.NgayThamGia</div>
                                </li>
                                <li>
                                    <div class="title">Ngày Kết thúc</div>
                                    <div class="text">@Model.bHXHs?.NgayKetThuc</div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 d-flex">
                    <div class="card profile-box flex-fill">
                        <div class="card-body">
                            <h3 class="card-title">Compassionate leave<a href="#" class="edit-icon" data-toggle="modal" data-target="#phepNam_infor_modal"><i class="fa fa-pencil"></i></a></h3>
                            <ul class="personal-info">
                                <li>
                                    <div class="title">Tổng Phép năm @Model.phepNams?.FirstOrDefault()?.Year</div>
                                    <div class="text">@Model.phepNams?.FirstOrDefault()?.SoPhepNam</div>
                                </li>
                                <li>
                                    <div class="title">Còn Lại</div>
                                    <div class="text">@Model.phepNams?.FirstOrDefault()?.SoPhepConLai</div>
                                </li>

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 d-flex">
                    <div class="card profile-box flex-fill">
                        <div class="card-body">
                            <h3 class="card-title">Quit work<a href="#" class="edit-icon" data-toggle="modal" data-target="#quitWork_infor_modal"><i class="fa fa-pencil"></i></a></h3>
                            <ul class="personal-info">
                                <li>
                                    <div class="title">Thời gian nghỉ việc</div>
                                    <div class="text">@Model.NgayNghiViec</div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 d-flex">
                    <div class="card profile-box flex-fill">
                        <div class="card-body">
                            <h3 class="card-title">Labor discipline<a href="#" class="edit-icon" data-toggle="modal" data-target="#LaborDiscipline_infor_modal"><i class="fa fa-pencil"></i></a></h3>
                            <ul class="personal-info">
                                <li>
                                    <div class="title">Kỷ Luật Lao Động</div>
                                    <div class="text">@Model.NgayNghiViec</div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /Profile Info Tab -->
        <!-- Projects Tab -->
        <div class="tab-pane fade" id="emp_projects">
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-md-12 col-xl-12">
                    <div class="table-responsive">
                        <table class="table table-striped custom-table">
                            <thead>
                                <tr>
                                    <th>Giấy tờ</th>
                                    <th class="text-center">Hoàn Thành/Chưa Hoàn Thành</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Sơ Yếu Lý Lịch</td>
                                    <td class="text-center">
                                        <input type="checkbox" checked="">
                                    </td>
                                </tr>
                                <tr>
                                    <td>CMTND/CCCD</td>
