@model ErrorInforModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@section Scripts{
    <script>
        $(document).ready(function () {
            $('#btnBack').on('click', function () {
                history.back();
            });
        });
    </script>
}
<div class="error-box">
    <h1>@Model.Code</h1>
    <h3><i class="fa fa-warning"></i> @Model.Title</h3>
    <p>@Model.Message</p>
    <a href="#" id="btnBack" class="btn btn-custom">Back to Home</a>
</div>


