                    .Widget(widget => widget.Button()
                    .Text("Close")
                    .OnClick(@<text>
                        function hideInfo(data) {

                        let _dataGridTmp = $("#gridContainer1").dxDataGrid("instance");
                        _dataGridTmp.refresh();

                        $('#txt_MaDangKy').val(0);
                        const popup = $("#chitiet_file_popup").dxPopup("instance");
